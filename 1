def leaf():
    pensize(2)
    fillcolor('green')
    begin_fill()
    circle(100,100)
    lt(79)
    circle(100,100)
    end_fill()
    color('black')
    lt(130)
    fd(150)
    penup()
    lt(180)
    fd(140)
    pendown()
    rt(140)
    fd(45)
    lt(180)
    fd(45)
    lt(95)
    fd(43)
    lt(180)
    fd(45)
    rt(135)
    fd(40)
    rt(45)
    fd(50)
    lt(180)
    fd(50)
    rt(90)
    fd(50)
    lt(180)
    fd(50)
    lt(135)
    fd(50)
    lt(50)
    fd(30)
    lt(180)
    fd(33)
    lt(90)
    fd(35)
leaf()
color('white')
penup()
rt(150)
fd(140)
pendown()
rt(120)
leaf()
color('white')
rt(13)
penup()
fd(340)
rt(90)
pendown()
hideturtle()
write("Will you go on a date with me?",font=('arial',20,'normal'))
done()
