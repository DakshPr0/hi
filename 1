from turtle import *
bgcolor('black')
pensize(3)
shape('turtle')
lt(50)
color('white')
fillcolor('red')
begin_fill()
circle(150,50)
rt(45)
fd(40)
lt(150)
fd(40)
lt(10)
circle(150,50)
lt(60)
end_fill()
penup()
fd(53)
pendown()
